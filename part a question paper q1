#include<stdio.h>
#include<math.h>
int main(){
    float x1,x2,y1,y2,distance;
    printf("enter the value of x1 and x2\n");
    scanf("%f %f",&x1,&x2);
    printf("enter the value of y1 and y2\n");
    scanf("%f %f",&y1,&y2);
    distance=((x2-x1)*(x2-x1)+(y2-y1)*(y2-y1));
    printf("the distance is = %.2f\n",distance);
}
